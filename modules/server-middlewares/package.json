    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.1",
    "passport": "0.3.2",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "2.2.1",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.6",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "@types/chai": "4.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^6.0.101",
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "mocha": "^6.2.0",
    "ts-node": "^3.3.0",
    "typescript": "^3.9.6",
    "rimraf": "^3.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
