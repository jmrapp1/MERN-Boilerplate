{
  "name": "@jrapp/server-middlewares",
  "version": "1.0.0",
  "description": "Server-side middlewares",
  "author": "Jon Rapp",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "test:server": "mocha --opts test/mocha.opts -r ts-node/register ./src/**/*.test.ts",
    "build": "rimraf ./dist && tsc -p ./",
