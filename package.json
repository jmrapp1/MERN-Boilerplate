{
  "name": "MERN_Boilerplate",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd ./src/client && webpack-dev-server --node-env=development --config ./webpack.config.dev.js",
    "dev:server": "concurrently \"tsc -w -p ./src/server\" \"nodemon --config ./src/server/nodemon.json dist/server/app.js\"",
    "prod": "npm run prod:server && npm run prod:client && npm run start",
    "prod:client": "cd ./src/client && webpack --node-env=production --config ./webpack.config.prod.js",
    "prod:server": "tsc -p ./src/server",
    "start": "node ./dist/server/app.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcryptjs": "^2.4.3",
