{
  "name": "React-Node-App",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev:server": "concurrently \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "test:server": "mocha $NODE_DEBUG_OPTION --opts test/mocha.opts -r ts-node/register server/**/*.test.ts",
    "lint": "ng lint",
    "predev": "tsc -p server",
    "dev": "concurrently \"node client/start-client.js\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "dev:debug": "concurrently \"node client/start-client.js\" \"tsc -w -p server\" \"nodemon --inspect dist/server/app.js\"",
    "start": "nodemon dist/server/app.js",
    "postinstall": "tsc -p server",
    "install": "npm run build-react",
    "build-css": "cd client && node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && cd client  node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-react": "npm-run-all -p watch-css start-js",
    "build-js": "cd client && react-scripts build",
    "build-react": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.17.2",
    "bootstrap": "^3.3.7",
    "core-js": "2.4.1",
    "cors": "^2.8.4",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "express": "4.15.3",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.0.11",
    "jquery": "^3.3.1",
    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.5",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-sass-chokidar": "0.0.3",
    "passport": "0.3.2",
    "passport-jwt": "2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-toastify": "^3.4.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.6",
    "typedi": "^0.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "^6.0.101",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "concurrently": "3.5.0",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "mocha-jsdom": "^1.1.0",
    "mocha-junit-reporter": "^1.13.0",
    "nodemon": "1.11.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.5.0",
    "protractor": "~5.1.2",
    "ts-node": "^3.0.6",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  },
  "engines": {
    "node": "6.11.2",
    "npm": "5.5.1"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "cache": true
  }
}
