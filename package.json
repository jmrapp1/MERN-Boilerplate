    "cors": "^2.8.4",
    "dotenv": "4.0.0",
    "email-validator": "^2.0.3",
    "eslint": "3.19.0",
    "express": "4.15.3",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.0.11",
    "jquery": "^3.3.1",
    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.5",
    "moment-timezone": "^0.5.16",
    "mongoose": "^5.0.5",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-sass-chokidar": "0.0.3",
    "passport": "0.3.2",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "2.2.1",
    "react": "^16.1.1",
